<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
        <entry key="IsParamsFile">false</entry>
        <entry key="PrintEveryX">100</entry>
        <entry key="PopulationSize">30</entry>
        <entry key="TestSetSize">15</entry>
        <entry key="MaxGenerations">20000</entry>
        <entry key="FitnessFunction">edu.utep.pw.ga.fitness.DifferentPairsFitness</entry>
        <entry key="CrossoverStrategy">edu.utep.pw.ga.crossover.SingleCrossover</entry>
        <entry key="ParentSelector">edu.utep.pw.ga.selection.RouletteWheelSelector</entry>
        <entry key="ReplacementSelector">edu.utep.pw.ga.replacement.WeakerIndividualsReplacement</entry>
        <entry key="MutationStrategy">edu.utep.pw.ga.mutation.SingleMutationRate</entry>
        <entry key="PopulationInitializer">edu.utep.pw.ga.initialization.RandomPopulationInitializer</entry>
        <entry key="MutationRate">1</entry>
        <entry key="NumberReproductions">15</entry>
        <entry key="ImmigrantEveryX">1</entry>
        <entry key="NumberCrossoverPoints">5</entry>
        
        <!-- Smart Mutations -->
        <entry key="UseSimilarityMutation">true</entry>
        <entry key="SimilarityMutationType">PROBABILITY</entry>
        <entry key="SimilarityMutationValue">0.01</entry>
        <entry key="ThresholdSimilarity">80</entry>
        <entry key="ReplaceWithSmartTestCase">true</entry>
        <entry key="UseValueOccurrenceMutation">true</entry>
        <entry key="ValueOccurrenceMutationType">PROBABILITY</entry>
        <entry key="ValueOccurrenceMutationValue">0.1</entry>
        <entry key="BalanceValueOccurences">false</entry>
        <entry key="UsePairOccurrenceMutation">true</entry>
        <entry key="PairOccurrenceMutationType">PROBABILITY</entry>
        <entry key="PairOccurrenceMutationValue">0.01</entry>
        <entry key="ThresholdPairOcurrences">8</entry>
</properties>